{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "links/index.html", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/sitemap.xml",
      "dest": "links/sitemap.xml",
      "headers": {
        "Content-Type": "application/xml"
      }
    },
    {
      "src": "/robots.txt",
      "dest": "links/robots.txt",
      "headers": {
        "Content-Type": "text/plain"
      }
    },
    { "src": "/(.*)", "dest": "links/$1" }
  ]
}
